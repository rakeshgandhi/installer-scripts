[Unit]
Description=Doctor Payout Master Service
After=network.target

[Service]
WorkingDirectory=/opt/local/share/DoctorPayout-MasterService
ExecStart=/usr/bin/dotnet /opt/local/share/DoctorPayout-MasterService/publish/MasterService.API.dll
Restart=always
RestartSec=5
User=www-data
Environment=DOTNET_ENVIRONMENT=uat
Environment=ASPNETCORE_URLS=http://127.0.0.1:7264

[Install]
WantedBy=multi-user.target
