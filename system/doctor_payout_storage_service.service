[Unit]
Description=Doctor Payout Storage Service
After=network.target

[Service]
WorkingDirectory=/opt/local/share/DoctorPayout-StorageService
ExecStart=/usr/bin/dotnet /opt/local/share/DoctorPayout-StorageService/publish/StorageService.API.dll
Restart=always
RestartSec=5
User=www-data
Environment=DOTNET_ENVIRONMENT=uat
Environment=ASPNETCORE_URLS=http://127.0.0.1:7125

[Install]
WantedBy=multi-user.target
