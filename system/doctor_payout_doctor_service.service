[Unit]
Description=Doctor Payout Doctor Service
After=network.target

[Service]
WorkingDirectory=/opt/local/share/DoctorPayout-DoctorService
ExecStart=/usr/bin/dotnet /opt/local/share/DoctorPayout-DoctorService/publish/DoctorService.API.dll
Restart=always
RestartSec=5
User=www-data
Environment=DOTNET_ENVIRONMENT=uat
Environment=ASPNETCORE_URLS=http://127.0.0.1:7272

[Install]
WantedBy=multi-user.target
