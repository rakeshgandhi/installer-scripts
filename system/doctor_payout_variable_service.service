[Unit]
Description=Doctor Payout Variable Service
After=network.target

[Service]
WorkingDirectory=/opt/local/share/DoctorPayout-VariableService
ExecStart=/usr/bin/dotnet /opt/local/share/DoctorPayout-VariableService/publish/VariableService.API.dll
Restart=always
RestartSec=5
User=www-data
Environment=DOTNET_ENVIRONMENT=uat
Environment=ASPNETCORE_URLS=http://127.0.0.1:7267

[Install]
WantedBy=multi-user.target
