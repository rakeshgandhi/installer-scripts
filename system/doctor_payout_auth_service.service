[Unit]
Description=Doctor Payout Auth Service
After=network.target

[Service]
WorkingDirectory=/opt/local/share/DoctorPayout-AuthService
ExecStart=/usr/bin/dotnet /opt/local/share/DoctorPayout-AuthService/publish/AuthService.API.dll
Restart=always
RestartSec=5
User=www-data
Environment=DOTNET_ENVIRONMENT=uat
Environment=ASPNETCORE_URLS=http://127.0.0.1:7261

[Install]
WantedBy=multi-user.target
